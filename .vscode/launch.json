  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "attach",
      "name": "lambda",
      "port": 9230,
      "localRoot": "${workspaceFolder}",
      "remoteRoot": "/var/task",
      "restart": true,
      "soruceMaps": true,
      "skipFiles": [
        "<node_internals>/**",
        "/var/runtime/**",
        "**/node_modules/**"
      ],
      "continueOnAttach": false
    },
  ]
}